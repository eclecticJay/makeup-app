<h2><strong>New Artist</strong></h2>
<%= form_tag '/makeup_artists', method: :post do  %>
  <div class="row">
    <div class= "col-md-4">
      <%= label_tag :first_name %>
      <br>
      <%= text_field_tag :first_name %>
    </div>  
  </div>

  <div class="row">
    <div class= "col-md-4">
      <%= label_tag :last_name %> 
      <br>
      <%= text_field_tag :last_name %>
    </div>  
  </div>

  <div class="row">
    <div class= "col-md-4">
      <%= label_tag :email %> 
      <br>
      <%= text_field_tag :email %>
    </div>  
  </div>

  <div class="row">
    <div class= "col-md-4">
      <%= label_tag :phone %>
      <br> 
      <%= text_field_tag :phone %>
    </div>  
  </div>

  <div class="row">
    <div class= "col-md-4">
      <%= label_tag :zipcode %>
      <br>  
      <%= text_field_tag :zipcode %>
    </div>  
  </div>

  <div class = "row">
    <div class= "col-md-4">
      <%= label_tag :password %>
      <br>
      <%= password_field_tag :password %>
    </div>
  </div>

  <div class = "row">
    <div class= "col-md-4">
      <%= label_tag :password_confirmation %>
      <br>
      <%= password_field_tag :password_confirmation %>
    </div>
  </div>

  <div>
  <%= submit_tag "Submit", :class => "btn btn-info" %>
  </div>

<% end %>